{
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "match_about_blank": true,
            "all_frames": true,
            "run_at": "document_start",
            "js": [
                "inject.js"
            ]
        }
    ],
    "host_permissions": [
        ":///*"
    ],
    "manifest_version": 3,
    "name": "Proxy Manager Timezone",
    "permissions": [
        "storage",
        "webNavigation"
    ],
    "version": "1.570.247"
}